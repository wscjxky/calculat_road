/* accordant */
(function ($) {
    "use strict";

    function acc() {
        var $accmenu = $(".acc");
        $accmenu.addClass("level-1");
        $accmenu.children("li").children("ul").addClass("level-2");
        $accmenu.children("li").children("ul").children("li").children("ul").addClass("level-3");
        $accmenu.children("li").children("ul").children("li").children("ul").children("li").children("ul").addClass("level-4");
        $accmenu.children("li").children("ul").children("li").children("ul").children("li").children("ul").children("li").children("ul").addClass("level-5");
        $accmenu.find("a").next("ul").addClass("sub-menu");
        $accmenu.find("a").next("ul").parent("li").addClass("has-submenu");
        $accmenu.find("ul").css({
            "height": "auto",
            "display": "none"
        });
        Innovate();
        $(".largeClient").css({
            "display": "block"
        });
        $accmenu.on("mouseover", "a", function () {
            if ($(this).parent("li").hasClass("innovate-color")) {
                $(this).siblings('ul').css({
                    "display": "flex"
                })
            }
            $(this).parent("li").addClass("active-color");
            $(this).parent("li").next("li").addClass("hideborder");
            $(this).parent("li").siblings("li").removeClass("active-color");
            $(this).parent("li").siblings("li").removeClass("hideborder");
            $(this).parent("li").next("li").addClass("hideborder");
            $(this).not(".level-1>li>a").addClass("acc-active");
            $(this).not(".level-1>li>a").parent("li").siblings("li").find("a").removeClass("acc-active");
            $(this).next(".sub-menu").show();
            $(this).parent("li").siblings("li").find(".sub-menu").hide();
            var heights = [];
            $(this).parents(".level-1").find(".sub-menu").each(function () {
                $(".sub-menu").css({
                    "height": "auto"
                });
                if (!$(this).is(":hidden")) {
                    var num = $(this).height();
                    heights.push(num)
                }
            });
            var a = Math.max.apply(null, heights);
            $(this).parents(".level-1").find(".sub-menu").height(a);
            $accmenu.height(a + 60)
        });
        $accmenu.find(".level-2").on("mouseleave", function () {
            $accmenu.css({
                "height": "auto"
            });
            $(".level-1 .sub-menu").hide();
            $(this).parent("li").removeClass("active-color");
            $(this).parent("li").next("li").removeClass("hideborder");
            $accmenu.find("a").removeClass("popup").removeClass("acc-active")
        });
        $accmenu.on("mouseleave", function () {
            $accmenu.css({
                "height": "auto"
            });
            $(".level-1 .sub-menu").hide();
            $(this).children("li").removeClass("active-color");
            $(this).children("li").next("li").removeClass("hideborder");
            $accmenu.find("a").removeClass("popup").removeClass("acc-active")
        })
    }

    function smallacc() {
        var $smallacc = $(".smallacc");
        $smallacc.addClass("level-1");
        $smallacc.children("li").children("ul").addClass("level-2");
        $smallacc.children("li").children("ul").children("li").children("ul").addClass("level-3");
        $smallacc.children("li").children("ul").children("li").children("ul").children("li").children("ul").addClass("level-4");
        $smallacc.children("li").children("ul").children("li").children("ul").children("li").children("ul").children("li").children("ul").addClass("level-5");
        $smallacc.find("a").next("ul").addClass("sub-menu");
        $smallacc.find("a").next("ul").parent("li").addClass("has-submenu");
        $smallacc.find("ul").css({
            "height": "auto",
            "display": "none"
        });
        InnovateSmall();
        $smallacc.find(".has-submenu").prepend('<div class="clickable"></div>');
        $smallacc.find(".has-submenu").children("a").append('<div class="plus-icon"></div>');
        $(".plus-icon").html('<svg viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg" width="200" height="200"><defs><style type="text/css">path{fill:#ccc}</style></defs><path d="M922.576565 474.354299l-371.566162 0.116401V101.40738c0-20.681253-16.785832-37.467086-37.469093-37.467086s-37.467086 16.785832-37.467086 37.467086v372.802421l-377.448427-0.208719h-0.112388c-20.645129 0-37.390823 16.673445-37.467085 37.356705-0.112387 20.68326 16.637321 37.505217 37.316567 37.579473l377.709327 0.20872v373.699511c0 20.719385 16.785832 37.469093 37.465078 37.469093 20.68326 0 37.469093-16.749708 37.469093-37.469093V549.406878l371.265125-0.118408h0.146504c20.647136 0 37.39283-16.673445 37.4711-37.356705 0.07827-20.681253-16.669431-37.50321-37.312553-37.577466z"></path></svg>');
        $(".plus-icon").html('<svg viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg" width="200" height="200"><defs><style type="text/css">path{fill:#ccc}</style></defs><path d="M922.576565 474.354299l-371.566162 0.116401V101.40738c0-20.681253-16.785832-37.467086-37.469093-37.467086s-37.467086 16.785832-37.467086 37.467086v372.802421l-377.448427-0.208719h-0.112388c-20.645129 0-37.390823 16.673445-37.467085 37.356705-0.112387 20.68326 16.637321 37.505217 37.316567 37.579473l377.709327 0.20872v373.699511c0 20.719385 16.785832 37.469093 37.465078 37.469093 20.68326 0 37.469093-16.749708 37.469093-37.469093V549.406878l371.265125-0.118408h0.146504c20.647136 0 37.39283-16.673445 37.4711-37.356705 0.07827-20.681253-16.669431-37.50321-37.312553-37.577466z"></path></svg>');
        $(".clickable").on("click", function () {
            if (!$(this).next("a").next(".sub-menu").hasClass("clicked")) {
                $(this).next("a").next(".sub-menu").stop().slideDown(300, function () {
                    $(this).addClass("clicked");
                    $(this).prev("a").children(".plus-icon").addClass("rotateplus")
                });
                $(this).parent("li").siblings("li").find(".sub-menu").removeClass("clicked");
                $(this).parent("li").siblings("li").find(".sub-menu").stop().slideUp();
                $(this).parent("li").siblings("li").find(".sub-menu").prev("a").children(".plus-icon").removeClass("rotateplus")
            } else {
                $(this).next("a").next(".sub-menu").stop().slideUp(300, function () {
                    $(this).removeClass("clicked");
                    $(this).prev("a").children(".plus-icon").removeClass("rotateplus")
                })
            }
        });
        $(".togglebtn").on("click", function () {
            if (!$(this).hasClass("activebtn")) {
                $(this).addClass("activebtn");
                $(".small-nav").addClass("shownav")
                $("body").addClass("show-mobile-nav");
                if ($("body").hasClass("show-lang-box")) {
                  $("body").removeClass("show-lang-box");
               }
               if ($("body").hasClass("show-mobile-search")) {
                  $("body").removeClass("show-mobile-search");
               }
            } else {
                $(this).removeClass("activebtn");
                $("body").removeClass("show-mobile-nav")
                $(".small-nav").removeClass("shownav");
                $(".small-nav").find(".sub-menu").removeClass("clicked").slideUp(100);
                $(".plus-icon").removeClass("rotateplus");
            }
        })
    }

    function LoadMenu() {
        acc();
        smallacc()
    }

    function Innovate() {
        var $accmenu = $(".acc");
        var $innovate = $accmenu.children('li').children('a');
        for (var i = 0; i < $innovate.length; i++) {
            if ($innovate[i].innerHTML == "创新融合") {
                var $innovateColor = $($innovate[i]);
                $innovateColor.parent('li').addClass("innovate-color");
            }
        }
    }

    function InnovateSmall() {
        var $smallacc = $(".smallacc");
        var $innovate = $smallacc.children('li').children('a');
        for (var i = 0; i < $innovate.length; i++) {
            if ($innovate[i].innerHTML == "创新融合") {
                var $innovateColor = $($innovate[i]);
                $innovateColor.parent('li').addClass("innovate-color-small");
            }
        }
    }
    $(function () {
        LoadMenu();
        Innovate()
    })
})(jQuery);
function detectIECompatibility() {
    var browser = navigator.appName
    var b_version = navigator.appVersion
    var version = b_version.split(";");
    if (typeof version[1] !== 'undefined') {

        var trim_Version = version[1].replace(/[ ]/g, "");
        if ((browser == "Microsoft Internet Explorer") && ((trim_Version == "MSIE6.0") || (trim_Version == "MSIE7.0") || (trim_Version == "MSIE8.0") || (trim_Version == "MSIE9.0"))) {
            $("body").prepend('<div class="compatibility_note"><div><p class="note">您的浏览器版本太低，建议您使用现代浏览器。<br>或者您可以使用手机扫描下方二维码浏览移动客户端页面。</p><img src="https://www.neusoft.com/cn/upload/images/neusoft-qrcode.gif"/><p class="thanks">感谢您对算路科技的支持！</p><div></div>');
            $(".compatibility_note").css({
                "width": "100%",
                "height": "100%",
                "position": "fixed",
                "z-index": 999999999999999999999999999999999999999999999999999,
                "top": 0,
                "left": 0,
                "background-color": "#ffffff"
            });
            $(".compatibility_note div").css({
                "width": "400px",
                "height": "260px",
                "position": "absolute",
                "z-index": 10,
                "top": "50%",
                "left": "50%",
                "margin": "-130px 0 0 -200px"
            });
            $(".compatibility_note p.note").css({
                "font-size": "14px",
                "text-align": "center"
            });
            $(".compatibility_note p.thanks").css({
                "font-size": "24px",
                "text-align": "center",
                "font-weight": "bold"
            });
            $(".compatibility_note img").css({
                "width": "150px",
                "height": "150px",
                "margin": "20px auto"
            });
        }

    }
}

function jsAnchors() {
    if ($(".platform-items").hasClass("js-anchors")) {
        $(".platform-items .col").on("click",
            function () {
                $link = $(this).find("a").attr("href");
                location = $link;
            });
    }
}

function detectBrowserType() {
   var browser = {
      versions: function () {
         "use strict";
         var u = navigator.userAgent;
         return {
               mobile: !!u.match(/AppleWebKit.*Mobile.*/),
               ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/),
               android: u.indexOf('Android') > -1 || u.indexOf('Adr') > -1,
         };
      }()
   };
   if (browser.versions.mobile || browser.versions.android || browser.versions.ios) {
      $(".header").find(".language").children("a").unbind("click").click(function () {
         if (!$("body").hasClass("show-lang-box")) {
            $("body").addClass("show-lang-box");

            /* nav */
            var tbtn = $(".togglebtn");
            if (tbtn.hasClass("activebtn")) {
               tbtn.removeClass("activebtn");
               $(".small-nav").removeClass("shownav");
               $("body").removeClass("show-mobile-nav")
               $(".small-nav").find(".sub-menu").removeClass("clicked").slideUp(100);
               $(".plus-icon").removeClass("rotateplus");
            }

            /* search */
            if ($("body").hasClass("show-mobile-search")) {
               $("body").removeClass("show-mobile-search");
            }

         } else {
            $("body").removeClass("show-lang-box");
         }
      });

      $(".search-toggle").unbind("click").click(function () {
         if (!$("body").hasClass("show-mobile-search")) {
               $("body").addClass("show-mobile-search");
               var tbtn = $(".togglebtn");
               if (tbtn.hasClass("activebtn")) {
                  tbtn.removeClass("activebtn");
                  $(".small-nav").removeClass("shownav");
                  $("body").removeClass("show-mobile-nav")
                  $(".small-nav").find(".sub-menu").removeClass("clicked").slideUp(100);
                  $(".plus-icon").removeClass("rotateplus");
               }
               if ($("body").hasClass("show-lang-box")) {
                  $("body").removeClass("show-lang-box");
               }

         } else {
            $("body").removeClass("show-mobile-search");
         }
      });

   } else {
      $(".header").find(".language").hover(function () {
         $("body").addClass("show-lang-box");
         if($("body").hasClass("show-lang-box")){
            $("body").removeClass("show-mobile-nav")
         }
         $(".togglebtn").removeClass("activebtn");
         $(".small-nav ").removeClass("shownav");
      },
      function () {
         $("body").removeClass("show-lang-box");
      });
   }
}

function removeEmptyP() {
    $(".pagename p").each(function () {
        var $this = $(this);
        if (($this.text() == "") || ($this.html() == "&nbsp;")) {
            $this.remove();
        }
    });
}

$(function () {
    detectBrowserType();
    detectIECompatibility();
    removeEmptyP();
    jsAnchors();
});

function dpxyc() {
    "use strict";
    $(".dpxyc").css({
        "margin-top": function () {
            var domtop = $(this).outerHeight() * 0.5;
            return - domtop
        },
        "margin-left": function () {
            var domleft = $(this).outerWidth() * 0.5;
            return - domleft
        }
    })
}

function dpyc() {
    "use strict";
    $(".dpyc").css({
        "margin-top": function () {
            var domtop = $(this).height() * 0.5;
            return - domtop
        }
    })
}

function dpxc() {
    "use strict";
    $(".dpxc").css({
        "margin-left": function () {
            var domleft = $(this).width() * 0.5;
            return - domleft
        }
    })
}

function dgib() {
    "use strict";
    $(".dgib").css({
        "background-image": function () {
            var bgimg = $(this).children("img").attr("src");
            var src = "url(" + bgimg + ")";
            return src
        }
    })
}

function getheight() {
    "use strict";

    var cd = $(".main-part-dom .main-part-dom-left li:nth-child(2)").css("display");
    if (cd === "none") {
        $(".main-part-dom .main-part-dom-left li:nth-child(1)").removeAttr("style");
    } else {
        $(".main-part-dom .main-part-dom-left li:nth-child(1)").css({

            "height": function () {

                var hei = $(".main-part-dom .main-part-dom-left li:nth-child(2)").height();

                return hei;

            }

        });
    }
}

function tabs() {
    var tabs = $('.tabs');
    tabs.each(function () {
        var tab = $(this),
            tabItems = tab.find('ul.tabs-navigation'),
            tabContentWrapper = tab.children('ul.tabs-content'),
            tabNavigation = tab.find('nav');
        tabItems.on('click', 'a',
            function (event) {
                event.preventDefault();
                var selectedItem = $(this);
                if (!selectedItem.hasClass('selected')) {
                    var selectedTab = selectedItem.data('content'),
                        selectedContent = tabContentWrapper.find('li[data-content="' + selectedTab + '"]');
                    var slectedContentHeight;
                    tabItems.find('a.selected').removeClass('selected');
                    selectedItem.addClass('selected');
                    selectedContent.addClass('selected').siblings('li').removeClass('selected');
                    setTimeout(function () {
                        slectedContentHeight = selectedContent.innerHeight();
                        if (tabNavigation.height() < slectedContentHeight) {
                            // slectedContentHeight = slectedContentHeight;
                        } else {
                            slectedContentHeight = tabNavigation.height() + 5;
                        }
                        tabContentWrapper.stop().animate({
                            'height': slectedContentHeight
                        },
                            300);
                    },
                        1);

                }
            });

        if (tabItems.width() > tabContentWrapper.width()) {
            if (!tab.hasClass("breaktab")) {
                tab.addClass("breaktab");
            } else {
                return false;
            }
        }

        var $th = tab.find("nav").height() + 5;
        tab.find('.tabs-content').css({
            "height": "auto",
            "min-height": $th
        });

    });

    $(window).on('resize',
        function () {
            tabs.each(function () {
                var tab = $(this);
                var $th = tab.find("nav").height() + 5;
                tab.find('.tabs-content').css({
                    "height": "auto",
                    "min-height": $th
                });
            });
            var tabItems = tabs.find('ul.tabs-navigation'),
                tabContentWrapper = tabs.children('ul.tabs-content');
            var navitems = tabItems.find("li");
            var fullLength = 0;
            navitems.each(function () {
                fullLength += $(this).width();
            });
            if (fullLength > tabContentWrapper.width()) {
                if (!tabs.hasClass("breaktab")) {
                    tabs.addClass("breaktab");
                } else {
                    return false;
                }
            } else {
                if (tabs.hasClass("breaktab")) {
                    tabs.removeClass("breaktab");
                } else {
                    return false;
                }
            }
        });
}

function pagetop() {
    $(".bkt").on("click",
        function () {
            $('body,html').animate({
                scrollTop: 0
            },
                1600, 'easeOutQuint');
            return false
        })
}

function pagetopshow() {
    var backtop = $(".bkt");
    if ($(this).scrollTop() > 300) {
        backtop.stop().fadeIn(300)
    } else {
        backtop.stop().fadeOut(300)
    }
}

function bannerjs() {
    "use strict";
    if ($(".slider").length) {
        $(".featured-main").eq(0).addClass("do");
        $(".slider").simpleSlider({
            slides: '.slide',
            swipe: false,
            magneticSwipe: true,
            transition: "fade",
            slideTracker: true,
            slideTrackerID: 'indexbannermenu',
            slideOnInterval: true,
            interval: 7000,
            animateDuration: 500,
            animationEasing: 'easeInOut',
            pauseOnHover: false,
            useDefaultCSS: false,
            neverEnding: true
        });

        $(".slider").on("beforeSliding",
            function (event) {
                var prevSlide = event.prevSlide;
                $(".featured-main").eq(prevSlide).removeClass("do");
            });

        $(".slider").on("afterSliding",
            function (event) {
                var newSlide = event.newSlide;
                $(".featured-main").eq(newSlide).addClass("do");

            });
    }
}

function productmenuclick() {
    "use strict";
    $(".product-menu > div > ul > li > div > a").click(function () {
        if ($(this).hasClass("on")) {
            $(this).next("ul").slideUp();
            $(this).removeClass("on");
            $(this).parent("div").removeClass("selected");
        } else {
            $(this).next("ul").slideDown();
            $(this).addClass("on");
            $(this).parent("div").addClass("selected");
        }
    });
}

function addTableWrapper() {
    if ($("body").hasClass("news")) {
        if ($("table").length) {
            $("table").each(function () {
                $(this).wrap('<div class="tabel-wrapper"></div>');
            });
        }
    }
}
$(document).ready(function () {
    "use strict";
    dpxyc();
    dpyc();
    dpxc();
    dgib();
    getheight();
    tabs();
    bannerjs();
    pagetop();
    productmenuclick();
    addTableWrapper();

    if ($(".bglightgray").css("background-image") == "none") {
        $(".pagename").remove();
    } else {
        $(".post-entry>h1").remove();
    };
});

$(window).on("scroll load",
    function () {
        "use strict";
        pagetopshow();
    });
$(window).bind("resize load",
    function () {
        "use strict";
        dpxyc();
        dpyc();
        dpxc();
        getheight();
    });

